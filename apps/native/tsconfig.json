{
  "extends": [
    "expo/tsconfig.base",
    "@zenncore/config/typescript/native"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/assets/*": [
        "./assets/*"
      ]
    }
  },
  "include": [
    "*.ts",
    "src",
    "tailwind.config.js",
    "@types/i18next.d.ts",
    ".expo/types/**/*.ts",
    "nativewind-env.d.ts",
    "expo-env.d.ts"
  ]
}
